{
  "design_system": {
    "style": "modern_playful",
    "color_scheme": {
      "background": {
        "primary": "#FFEEC9",
        "header": "#D6EFFF",
        "story": "rgba(44, 103, 82, 0.10)",
        "products": "#FFEEC9",
        "footer": "#FFEBF0"
      },
      "foreground": {
        "primary": "#87473F",
        "secondary": "#FFFFFF"
      },
      "accents": {
        "primary": "#FFC344",
        "secondary": "#F35C37"
      },
      "cards": {
        "background": "#FFFFFF",
        "foreground": "#87473F",
        "glass_effect": "rgba(255, 255, 255, 0.65)"
      }
    },
    "typography": {
      "headlines": {
        "font_family": "sans-serif",
        "font_weight": "bold",
        "font_size": "48px-64px",
        "text_transform": "uppercase",
        "color": "#FFFFFF",
        "accent_color": "#FFC344"
      },
      "subheadings": {
        "font_family": "sans-serif",
        "font_weight": "medium",
        "font_size": "24px-32px",
        "text_transform": "none",
        "color": "#FFC344"
      },
      "body_text": {
        "font_family": "sans-serif",
        "font_weight": "regular",
        "font_size": "16px-18px",
        "color": "#87473F"
      },
      "decorative_text": {
        "font_family": "sans-serif",
        "font_weight": "bold",
        "font_size": "32px-48px",
        "text_transform": "uppercase",
        "color": "#F35C37",
        "secondary_color": "#87473F"
      }
    },
    "layout": {
      "structure": "vertical_scroll",
      "sections": [
        {
          "name": "header",
          "background": "#D6EFFF",
          "elements": [
            {
              "type": "logo",
              "position": "top_left",
              "color": "#87473F"
            },
            {
              "type": "navigation",
              "position": "top_right",
              "color": "#FFFFFF",
              "hover_color": "#FFC344"
            },
            {
              "type": "headline",
              "position": "center",
              "style": "headlines"
            },
            {
              "type": "hero_image",
              "border": {
                "style": "scalloped",
                "position": "bottom",
                "color": "#87473F"
              }
            },
            {
              "type": "cta_button",
              "text": "SHOP NOW",
              "background": "#FFC344",
              "foreground": "#2C6752",
              "hover_ring": "#FFC344"
            }
          ]
        },
        {
          "name": "story",
          "background": "rgba(44, 103, 82, 0.10)",
          "elements": [
            {
              "type": "text_block",
              "position": "center",
              "color": "#FFFFFF"
            },
            {
              "type": "tag",
              "text": "OUR STORY",
              "background": "#FFC344",
              "foreground": "#2C6752",
              "shape": "rounded_rectangle"
            }
          ],
          "border": {
            "style": "scalloped",
            "position": "top_bottom",
            "color": "#FFFFFF"
          }
        },
        {
          "name": "products",
          "background": "#FFEEC9",
          "elements": [
            {
              "type": "headline",
              "position": "center",
              "style": "headlines",
              "color": "#87473F"
            },
            {
              "type": "product_cards",
              "layout": "horizontal_grid",
              "count": 4,
              "background": "#FFFFFF",
              "foreground": "#87473F",
              "shadow": "subtle",
              "hover_effect": "lift",
              "hover_color": "rgba(243, 92, 55, 0.10)"
            },
            {
              "type": "cta_link",
              "text": "SHOP ALL",
              "position": "top_right",
              "color": "#F35C37",
              "hover_color": "#FFC344"
            }
          ]
        },
        {
          "name": "footer",
          "background": "#FFEBF0",
          "elements": [
            {
              "type": "headline",
              "position": "center",
              "style": "decorative_text"
            },
            {
              "type": "subtext",
              "position": "center",
              "color": "#87473F",
              "icons": {
                "shape": "circle",
                "color": "#2C6752"
              }
            },
            {
              "type": "product_image",
              "border": {
                "style": "scalloped",
                "position": "top",
                "color": "#87473F"
              }
            }
          ]
        }
      ]
    },
    "decorative_elements": {
      "borders": {
        "style": "scalloped",
        "color": "#87473F"
      },
      "sparkles": {
        "shape": "star",
        "color": "#FFC344",
        "size": "small"
      },
      "icons": {
        "shape": "circle",
        "color": "#2C6752"
      }
    },
    "imagery": {
      "lifestyle": {
        "style": "playful_family",
        "lighting": "soft",
        "position":2 "header"
      },
      "product": {
        "style": "clean",
        "background": "#FFFFFF",
        "position": "products_footer"
      }
    },
    "spacing": {
      "padding": "40px-60px",
      "margins": "20px-30px",
      "alignment": "center"
    },
    "interactive_elements": {
      "buttons": {
        "shape": "rounded",
        "background": "#FFC344",
        "foreground": "#2C6752",
        "hover_effect": "ring",
        "hover_color": "#FFC344"
      },
      "links": {
        "style": "underlined",
        "color": "#F35C37",
        "hover_color": "#FFC344"
      }
    }
  }
}